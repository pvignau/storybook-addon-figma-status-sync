{
  "name": "storybook-addon-figma-status-sync",
  "version": "0.0.0",
  "description": "A Storybook addon to sync Figma component status as Storybook tags ",
  "keywords": [
    "storybook",
    "storybook-addons",
    "figma",
    "design-system",
    "design",
    "organize",
    "tags",
    "status"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pvignau/storybook-figma-status-sync.git"
  },
  "type": "module",
  "license": "MIT",
  "author": "Pierre-Yves Vignau <vignau.py@gmail.com>",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./preview": {
      "types": "./dist/index.d.ts",
      "import": "./dist/preview.js",
      "require": "./dist/preview.cjs"
    },
    "./preset": "./dist/preset.cjs",
    "./manager": "./dist/manager.js",
    "./package.json": "./package.json"
  },
  "bin": {
    "figmasync": "./dist/figmasync.cjs"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "build:storybook": "storybook build",
    "build:watch": "pnpm build --watch",
    "clean": "rimraf ./dist",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint --cache .",
    "lint:fix": "pnpm lint --fix",
    "prebuild": "pnpm clean",
    "prepack": "pnpm build",
    "prepare": "husky",
    "prerelease": "zx scripts/prepublish-checks.js",
    "release": "pnpm build && pnpm semantic-release",
    "start": "run-p build:watch \"storybook --quiet\"",
    "storybook": "storybook dev -p 6006",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@storybook/icons": "^1.2.12",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.15.0",
    "@storybook/addon-essentials": "^8.0.0",
    "@storybook/addon-interactions": "^8.0.0",
    "@storybook/addon-links": "^8.0.0",
    "@storybook/blocks": "^8.0.0",
    "@storybook/react": "^8.0.0",
    "@storybook/react-vite": "^8.0.0",
    "@storybook/test": "^8.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-istanbul": "2.1.6",
    "auto": "^11.3.0",
    "boxen": "^8.0.1",
    "commitlint": "^19.6.0",
    "dedent": "^1.5.3",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "npm-run-all2": "^7.0.1",
    "prettier": "^3.4.1",
    "prompts": "^2.4.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.2.0",
    "storybook": "^8.0.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0",
    "vite": "^6.0.1",
    "vitest": "^2.1.6",
    "zx": "^8.2.4"
  },
  "peerDependencies": {
    "storybook": "^8.0.0"
  },
  "overrides": {
    "storybook": "^8.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "exportEntries": [
      "src/index.ts"
    ],
    "managerEntries": [
      "src/manager.tsx"
    ],
    "previewEntries": [
      "src/preview.ts"
    ],
    "nodeEntries": [
      "src/preset.ts"
    ]
  },
  "packageManager": "pnpm@9.12.3",
  "engines": {
    "node": ">=20"
  },
  "storybook": {
    "displayName": "Figma Status Sync",
    "supportedFrameworks": [
      "supported-frameworks"
    ],
    "icon": "https://raw.githubusercontent.com/pvignau/storybook-figma-status-sync/main/static/icon.png"
  }
}
